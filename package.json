    "@emotion/styled": "^11",
    "framer-motion": "^4",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "marked": "^3.0.2",
    "next": "11.1.2",
    "next-themes": "^0.0.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^7.0.1",
    "react-material-ui-carousel": "https://github.com/SkyBlockDev/react-material-ui-carousel",
    "react-slick": "^0.28.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.3",
    "@types/marked": "^3.0.0",
    "@types/react": "17.0.21",
    "@types/react-slick": "^0.23.5",
    "autoprefixer": "^9",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "postcss": "^7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "4.4.3"
  },
  "prettier": {
    "printWidth": 6000
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  }
